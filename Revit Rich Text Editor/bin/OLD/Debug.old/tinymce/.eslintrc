{
	"env": {
		"browser": true,
		"amd": true
	},

	"globals": {
		"escape": true,
		"unescape": true
	},

	"rules": {
		"quotes": 0,
		"camelcase": 0,
		"strict": 0,
		"eqeqeq": 0,
		"no-underscore-dangle": 0,
		"consistent-return": 0,
		"no-shadow": 0,
		"no-use-before-define": 0,
		"brace-style": [2, "1tbs"],
		"func-style": [2, "declaration"],
		"guard-for-in": 0,
		"no-floating-decimal": 2,
		"no-nested-ternary": 2,
		"radix": 2,
		"wrap-iife": [2, "inside"],
		"consistent-this": [2, "self"],
		"no-bitwise": 2,
		"max-params": [2, 50],
		"max-depth": [1, 8],
		"max-statements": [2, 150],
		"max-len": [1, 140, 4],
		"eol-last": 0
	}
}